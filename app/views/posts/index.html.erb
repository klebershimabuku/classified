
<div class="row9">
  <div class='pull-left'>
    <div class="page-header">
      <h1>Meus anúncios</h1>
    </div>
  </div>
  <div class='pull-right'>
    <%= link_to 'Novo anúncio', new_post_path, :class => 'btn btn-primary' %>
  </div>
</div>

<div class='clear'></div>
<div class='feeds'>
  <% if @posts.any? %>
    <%= render partial: 'shared/post', collection: @posts %>
  <% end %>

  <%= paginate @posts %>
</div>
